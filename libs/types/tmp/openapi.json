{
  "components": {
    "schemas": {
      "AccountPreferences": {
        "properties": {
          "blockedUsers": {
            "items": {
              "format": "uuid",
              "type": "string"
            },
            "type": "array"
          },
          "editor": {
            "type": "object"
          },
          "preferredLanguage": {
            "nullable": true,
            "type": "string"
          },
          "theme": {
            "enum": [
              "dark",
              "light"
            ],
            "nullable": true,
            "type": "string"
          }
        },
        "title": "PreferencesPayload",
        "type": "object"
      },
      "AccountProfileUpdateRequest": {
        "properties": {
          "bio": {
            "maxLength": 500,
            "type": "string"
          },
          "location": {
            "maxLength": 100,
            "type": "string"
          },
          "picture": {
            "format": "uri",
            "type": "string"
          },
          "socials": {
            "items": {
              "format": "uri",
              "type": "string"
            },
            "maxItems": 5,
            "type": "array"
          }
        },
        "title": "ProfileUpdateRequest",
        "type": "object"
      },
      "AccountProfileUpdateResponse": {
        "properties": {
          "message": {
            "type": "string"
          },
          "profile": {
            "type": "object"
          }
        },
        "title": "ProfileUpdateResponse",
        "type": "object"
      },
      "AccountStatusResponse": {
        "properties": {
          "isAuthenticated": {
            "type": "boolean"
          },
          "role": {
            "type": "string"
          },
          "userId": {
            "format": "uuid",
            "type": "string"
          },
          "username": {
            "type": "string"
          }
        },
        "required": [
          "isAuthenticated"
        ],
        "title": "StatusResponse",
        "type": "object"
      },
      "AuthMessageResponse": {
        "properties": {
          "message": {
            "type": "string"
          }
        },
        "title": "MessageResponse",
        "type": "object"
      },
      "ErrorResponse": {
        "properties": {
          "error": {
            "type": "string"
          },
          "errors": {
            "type": "object"
          },
          "message": {
            "type": "string"
          }
        },
        "title": "ErrorResponse",
        "type": "object"
      },
      "LoginRequest": {
        "properties": {
          "identifier": {
            "description": "Username or email",
            "type": "string"
          },
          "password": {
            "format": "password",
            "type": "string"
          }
        },
        "required": [
          "identifier",
          "password"
        ],
        "title": "LoginRequest",
        "type": "object"
      },
      "RegisterRequest": {
        "properties": {
          "email": {
            "format": "email",
            "type": "string"
          },
          "password": {
            "format": "password",
            "minLength": 14,
            "type": "string"
          },
          "passwordConfirmation": {
            "format": "password",
            "type": "string"
          },
          "username": {
            "maxLength": 20,
            "minLength": 3,
            "type": "string"
          }
        },
        "required": [
          "username",
          "email",
          "password"
        ],
        "title": "RegisterRequest",
        "type": "object"
      }
    }
  },
