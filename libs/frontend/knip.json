{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"src/routes/**/*.{ts,svelte}",
		"src/lib/**/*.{ts,svelte}",
		"src/hooks.server.ts",
		"svelte.config.js",
		"vite.config.ts",
		"orval.config.ts"
	],
	"project": ["src/**/*.{ts,svelte}"],
	"ignore": [
		"build/**",
		".svelte-kit/**",
		"src/lib/api/generated/**",
		"**/*.spec.ts",
		"**/*.test.ts"
	],
	"ignoreDependencies": ["$lib/types"]
}
